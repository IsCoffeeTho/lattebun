{
	"name": "LatteBun",
	"plugin": [
		"typedoc-plugin-markdown",
		"typedoc-github-wiki-theme",
		"typedoc-plugin-rename-defaults"
	],
	"out": "./wiki",
	"entryPoints": [
		"./index.ts",
		"./src/*.ts",
		"./src/**/*.ts"
	],
	"projectDocuments": [
		"docs/**/*.md"
	],
	"exclude": [
		"node_modules/**/*.ts",
		"example/**/*"
	],
	"excludeInternal": true,
	"excludePrivate": true,
	"hidePageHeader": true,
	"propertyMembersFormat": "table",
	"enumMembersFormat": "table",
	"useCodeBlocks": true,
	"tableColumnSettings": {
		"hideDefaults": false
	},
	"sort": [
		"kind",
		"instance-first"
	]
}